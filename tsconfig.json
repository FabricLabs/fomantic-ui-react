{
  "compilerOptions": {
    "target": "esnext",
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "jsx": "react",
    "declaration": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    // "types": ["jest"],
    "strict": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true
  },
  "exclude": ["node_modules", "lib", "es", "dist", "typings", "**/__test__", "test"]
}
